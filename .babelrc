{
  "presets": [
    [
      "env",
      {
        "targets": {
          "node": "6",
          "browsers": "last 2 versions"
        },
        "useBuiltIns": true
      }
    ],
    "react"
  ],
  "plugins": [
    [ "module-resolver", { "root": [ "src" ] }],
    [ "transform-object-rest-spread", { "useBuiltIns": true }]
  ]
}
